{
  "name": "competence-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://nathabee.github.io/competence_project",
  "scripts": {
    "dev": "dotenv -e .env.local next dev",
    "local-build": "dotenv -e .env.local next build ",
    "local-start": "dotenv -e .env.local next start",
    "build": "dotenv -e .env.production next build",
    "start": "dotenv -e .env.production next start",
    "demo-test": "dotenv -e .env.demolocal next build &&  dotenv -e .env.demolocal npx serve out --single --prefix /",
    "demo-local-build": "rm -rf out && dotenv -e .env.demolocal -- npm run build && touch ./out/.nojekyll",
    "demo-local-start": "dotenv -e .env.demolocal npx serve out --single --prefix /",
    "demo-prod-build": "rm -rf out &&   dotenv -e  .env.demo npm run build   && touch ./out/.nojekyll",
    "demo-deploy": "rm -rf out &&   dotenv -e  .env.demo npm run build   && touch ./out/.nojekyll && gh-pages -d out -t true",
    "test": "dotenv -e .env.local jest",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "html2canvas": "^1.4.1",
    "i18next": "^23.16.4",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.2",
    "jwt-decode": "^4.0.0",
    "next": "^15.0.1",
    "nookies": "^2.5.2",
    "react": "^18",
    "react-bootstrap": "^2.10.5",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/axios-mock-adapter": "^1.10.4",
    "@types/bootstrap": "^5.2.10",
    "@types/i18next": "^13.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18",
    "@types/react-i18next": "^8.1.0",
    "@types/testing-library__jest-dom": "^6.0.0",
    "axios-mock-adapter": "^2.1.0",
    "babel-jest": "^29.7.0",
    "chart.js": "^4.4.5",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.14",
    "gh-pages": "^6.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.13",
    "ts-jest": "^29.2.5",
    "typescript": "^5"
  }
}
