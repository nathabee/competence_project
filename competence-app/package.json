{
  "name": "competence-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.local next dev",
    "local-build": "dotenv -e .env.local next build ",
    "local-start": "dotenv -e .env.local next start",
    "build": "dotenv -e .env.production next build",
    "start": "dotenv -e .env.production next start",
    "demo-build": "dotenv -e .env.demo next build",
    "obsolete-demo-move": "mkdir competence_project; mv out/*  competence_project; mv competence_project out",
    "obsolete-demo-start": "dotenv -e .env.demo npx serve out --single --prefix /competence_project",
    "demo-start": "dotenv -e .env.demo npx serve out --single  ",
    "demo-deploy": "gh-pages -d out",
    "lint": "next lint"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.2",
    "jwt-decode": "^4.0.0",
    "next": "^15.0.1",
    "nookies": "^2.5.2",
    "react": "^18",
    "react-bootstrap": "^2.10.5",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.10",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18",
    "chart.js": "^4.4.5",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.14",
    "gh-pages": "^6.2.0",
